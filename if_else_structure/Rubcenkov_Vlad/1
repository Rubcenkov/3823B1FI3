#include <stdio.h>
#include <stdlib.h>
int main()
{
    float ex_1, ex_2, ex_3, a;
    printf("scores\n");
    scanf("%f %f %f", &ex_1, &ex_2, &ex_3);
    a = (ex_1 + ex_2 + ex_3)/3;
    if a >= 4.5
        printf("Excellent");
    else{
        if a >= 3.5
            printf("Good");
        else
            printf("Bad");
    }
    return 0;
}
