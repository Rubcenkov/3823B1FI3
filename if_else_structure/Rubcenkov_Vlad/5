#include <stdio.h>
#include <stdlib.h>

void main() {
    double x, y, z;
    printf("Введите значение переменыых x, y, z: "); 
    scanf("%g %g %g", &x, &y, &z);
    if (x>=y && z>=x){
        printf("%g %g %g", y, x, z);}
    else if (x>=y && z<=y){
        printf("%g %g %g", z, y, x);}
    else if (x>=y && z>=y){
        printf("%g %g %g", y, z, x);}
    else if (y>=x && x<=z && y>=z){
        printf("%g %g %g", x, z, y);}
    else if (y>=x && x>=z){
        printf("%g %g %g", z, x, y);}
    else if (z>=y && y>=x){
        printf("%g %g %g", x, y, z);}
}
